Drop table Jabbarov;
CREATE TABLE Jabbarov (
    student_id INT PRIMARY KEY,
    name NVARCHAR2(30) NOT NULL,
    surname NVARCHAR2(30) NOT NULL,
    Email VARCHAR(100) UNIQUE,
    Fincode NVARCHAR2(30) UNIQUE
);

create TABLE Jabbarov (student_id int PRIMARY KEY, name NVARCHAR2(30) NOT NULL,surname NVARCHAR2(30) NOT NULL, Email VARCHAR(100) UNIQUE, Fincode NVARCHAR2(30) UNIQUE);
